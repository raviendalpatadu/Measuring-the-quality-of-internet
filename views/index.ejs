<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Speed Internet</title>
    <link rel="stylesheet" href="./css/style.css" />

    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC"
      crossorigin="anonymous"
    />

    <style></style>
  </head>
  <body>
    <div id="loader" class="loader">
      <img id="loading" src="./img/loading.gif" />
    </div>
    <div class="container-fluid vh-100">
      <nav class="navbar-lg bg-light p-0">
        <div class="container-fluid">
          <span class="navbar-brand fs-2 mt-2 h1">RUB C</span>
        </div>
      </nav>

      <div
        class="row-fluid row-cols-1 row-cols-md-1 d-flex align-items-center justify-content-center h-100 w-100"
      >
        <div class="col-12">
          <div class="row row-cols-1 row-cols-md-1">
            <div class="d-flex justify-content-center">
              <div>
                <h1>Check Your Internet Speed</h1>
                <p>
                  <button class="btn-danger" id="check">Check sSpeed</button>
                </p>
              </div>
            </div>
            <div class="row row-cols-1 row-cols-md-4 g-4" id="dataGroup">
              <div class="col" id="upSpeed">
                <div class="card">
                  <svg
                    xmlns="http://www.w3.org/2000/svg"
                    width="50"
                    height="50"
                    fill="currentColor"
                    class="card-img-top bi bi-upload"
                    viewBox="0 0 16 16"
                  >
                    <path
                      d="M.5 9.9a.5.5 0 0 1 .5.5v2.5a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1v-2.5a.5.5 0 0 1 1 0v2.5a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2v-2.5a.5.5 0 0 1 .5-.5z"
                    />
                    <path
                      d="M7.646 1.146a.5.5 0 0 1 .708 0l3 3a.5.5 0 0 1-.708.708L8.5 2.707V11.5a.5.5 0 0 1-1 0V2.707L5.354 4.854a.5.5 0 1 1-.708-.708l3-3z"
                    />
                  </svg>
                  <div class="card-body">
                    <h5 class="card-title d-flex justify-content-center">
                      Upload Speed
                    </h5>
                    <p
                      class="card-text d-flex justify-content-center"
                      id="upSpeed"
                    >
                      <span>---</span>
                    </p>
                  </div>
                </div>
              </div>
              <div class="col" id="downSpeed">
                <div class="card">
                  <svg
                    xmlns="http://www.w3.org/2000/svg"
                    width="50"
                    height="50"
                    fill="currentColor"
                    class="card-img-top bi bi-download"
                    viewBox="0 0 16 16"
                  >
                    <path
                      d="M.5 9.9a.5.5 0 0 1 .5.5v2.5a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1v-2.5a.5.5 0 0 1 1 0v2.5a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2v-2.5a.5.5 0 0 1 .5-.5z"
                    />
                    <path
                      d="M7.646 11.854a.5.5 0 0 0 .708 0l3-3a.5.5 0 0 0-.708-.708L8.5 10.293V1.5a.5.5 0 0 0-1 0v8.793L5.354 8.146a.5.5 0 1 0-.708.708l3 3z"
                    />
                  </svg>
                  <div class="card-body">
                    <h5 class="card-title d-flex justify-content-center">
                      Download Speed
                    </h5>
                    <p
                      class="card-text d-flex justify-content-center"
                      id="downSpeed"
                    >
                      <span>---</span>
                    </p>
                  </div>
                </div>
              </div>
              <div class="col" id="ping">
                <div class="card">
                  <svg
                    xmlns="http://www.w3.org/2000/svg"
                    width="50"
                    height="50"
                    fill="currentColor"
                    class="card-img-top bi bi-hourglass-split"
                    viewBox="0 0 16 16"
                  >
                    <path
                      d="M2.5 15a.5.5 0 1 1 0-1h1v-1a4.5 4.5 0 0 1 2.557-4.06c.29-.139.443-.377.443-.59v-.7c0-.213-.154-.451-.443-.59A4.5 4.5 0 0 1 3.5 3V2h-1a.5.5 0 0 1 0-1h11a.5.5 0 0 1 0 1h-1v1a4.5 4.5 0 0 1-2.557 4.06c-.29.139-.443.377-.443.59v.7c0 .213.154.451.443.59A4.5 4.5 0 0 1 12.5 13v1h1a.5.5 0 0 1 0 1h-11zm2-13v1c0 .537.12 1.045.337 1.5h6.326c.216-.455.337-.963.337-1.5V2h-7zm3 6.35c0 .701-.478 1.236-1.011 1.492A3.5 3.5 0 0 0 4.5 13s.866-1.299 3-1.48V8.35zm1 0v3.17c2.134.181 3 1.48 3 1.48a3.5 3.5 0 0 0-1.989-3.158C8.978 9.586 8.5 9.052 8.5 8.351z"
                    />
                  </svg>
                  <div class="card-body">
                    <h5 class="card-title d-flex justify-content-center">
                      Ping
                    </h5>
                    <p
                      class="card-text d-flex justify-content-center"
                      id="ping"
                    >
                      <span>---</span>
                    </p>
                  </div>
                </div>
              </div>
              <div class="col" id="packetLoss">
                <div class="card">
                  <svg
                    xmlns="http://www.w3.org/2000/svg"
                    width="50"
                    height="50"
                    fill="currentColor"
                    class="card-img-top bi bi-exclamation-circle"
                    viewBox="0 0 16 16"
                  >
                    <path
                      d="M8 15A7 7 0 1 1 8 1a7 7 0 0 1 0 14zm0 1A8 8 0 1 0 8 0a8 8 0 0 0 0 16z"
                    />
                    <path
                      d="M7.002 11a1 1 0 1 1 2 0 1 1 0 0 1-2 0zM7.1 4.995a.905.905 0 1 1 1.8 0l-.35 3.507a.552.552 0 0 1-1.1 0L7.1 4.995z"
                    />
                  </svg>
                  <div class="card-body">
                    <h5 class="card-title d-flex justify-content-center">
                      Packet Loss
                    </h5>
                    <p
                      class="card-text d-flex justify-content-center"
                      id="packetLoss"
                    >
                      <span>---</span>
                    </p>
                  </div>
                </div>
              </div>
            </div>
          </div>

          <div class="row row-cols-1 row-cols-md-2 g-2" id="dataGroup2">
            <div class="col d-flex justify-content-evenly" id="profile">
              <div class="card">
                <svg
                  xmlns="http://www.w3.org/2000/svg"
                  width="50"
                  height="50"
                  fill="currentColor"
                  class="card-img-top bi bi-person-circle"
                  viewBox="0 0 16 16"
                >
                  <path d="M11 6a3 3 0 1 1-6 0 3 3 0 0 1 6 0z" />
                  <path
                    fill-rule="evenodd"
                    d="M0 8a8 8 0 1 1 16 0A8 8 0 0 1 0 8zm8-7a7 7 0 0 0-5.468 11.37C3.242 11.226 4.805 10 8 10s4.757 1.225 5.468 2.37A7 7 0 0 0 8 1z"
                  />
                </svg>
                <div class="card-body">
                  <h5 class="card-title d-flex justify-content-center">
                    <%= ispName %>
                  </h5>
                  <p class="card-text d-flex justify-content-center">
                    <span><%= ipAddress %></span>
                  </p>
                </div>
              </div>
            </div>

            <div class="col d-flex justify-content-evenly" id="isp">
              <div class="card">
                <svg
                  xmlns="http://www.w3.org/2000/svg"
                  width="50"
                  height="50"
                  fill="currentColor"
                  class="card-img-top bi bi-globe2"
                  viewBox="0 0 16 16"
                >
                  <path
                    d="M0 8a8 8 0 1 1 16 0A8 8 0 0 1 0 8zm7.5-6.923c-.67.204-1.335.82-1.887 1.855-.143.268-.276.56-.395.872.705.157 1.472.257 2.282.287V1.077zM4.249 3.539c.142-.384.304-.744.481-1.078a6.7 6.7 0 0 1 .597-.933A7.01 7.01 0 0 0 3.051 3.05c.362.184.763.349 1.198.49zM3.509 7.5c.036-1.07.188-2.087.436-3.008a9.124 9.124 0 0 1-1.565-.667A6.964 6.964 0 0 0 1.018 7.5h2.49zm1.4-2.741a12.344 12.344 0 0 0-.4 2.741H7.5V5.091c-.91-.03-1.783-.145-2.591-.332zM8.5 5.09V7.5h2.99a12.342 12.342 0 0 0-.399-2.741c-.808.187-1.681.301-2.591.332zM4.51 8.5c.035.987.176 1.914.399 2.741A13.612 13.612 0 0 1 7.5 10.91V8.5H4.51zm3.99 0v2.409c.91.03 1.783.145 2.591.332.223-.827.364-1.754.4-2.741H8.5zm-3.282 3.696c.12.312.252.604.395.872.552 1.035 1.218 1.65 1.887 1.855V11.91c-.81.03-1.577.13-2.282.287zm.11 2.276a6.696 6.696 0 0 1-.598-.933 8.853 8.853 0 0 1-.481-1.079 8.38 8.38 0 0 0-1.198.49 7.01 7.01 0 0 0 2.276 1.522zm-1.383-2.964A13.36 13.36 0 0 1 3.508 8.5h-2.49a6.963 6.963 0 0 0 1.362 3.675c.47-.258.995-.482 1.565-.667zm6.728 2.964a7.009 7.009 0 0 0 2.275-1.521 8.376 8.376 0 0 0-1.197-.49 8.853 8.853 0 0 1-.481 1.078 6.688 6.688 0 0 1-.597.933zM8.5 11.909v3.014c.67-.204 1.335-.82 1.887-1.855.143-.268.276-.56.395-.872A12.63 12.63 0 0 0 8.5 11.91zm3.555-.401c.57.185 1.095.409 1.565.667A6.963 6.963 0 0 0 14.982 8.5h-2.49a13.36 13.36 0 0 1-.437 3.008zM14.982 7.5a6.963 6.963 0 0 0-1.362-3.675c-.47.258-.995.482-1.565.667.248.92.4 1.938.437 3.008h2.49zM11.27 2.461c.177.334.339.694.482 1.078a8.368 8.368 0 0 0 1.196-.49 7.01 7.01 0 0 0-2.275-1.52c.218.283.418.597.597.932zm-.488 1.343a7.765 7.765 0 0 0-.395-.872C9.835 1.897 9.17 1.282 8.5 1.077V4.09c.81-.03 1.577-.13 2.282-.287z"
                  />
                </svg>
                <div class="card-body">
                  <h5
                    class="card-title d-flex justify-content-center text-center"
                  >
                    <%= ispOrg %>
                  </h5>
                  <p class="card-text d-flex justify-content-center">
                    <span class="text-center"
                      ><%= city %> <br />
                      <%= region %> <br />
                      <%= country %></span
                    >
                  </p>
                </div>
              </div>
            </div>
          </div>
        </div>
        
      </div>
      <div class="container-fluid">
        <footer class="d-flex flex-wrap justify-content-between align-items-center py-3 my-4 border-top">
          <div class="col-md-4 d-flex align-items-center">
            <a href="/" class="mb-3 me-2 mb-md-0 text-muted text-decoration-none lh-1">
              <svg class="bi" width="30" height="24"><use xlink:href="#bootstrap"/></svg>
            </a>
            <span class="text-muted">&copy; 2022 IS1103 Group 17</span>
          </div>
      
          <ul class="nav col-md-4 justify-content-end list-unstyled d-flex">
            <li class="ms-3"><a class="text-muted" href="https://github.com/ravienDalpatadu/Measuring-the-quality-of-internet.git" target="_blank"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="currentColor" class="bi bi-github" viewBox="0 0 16 16">
              <path d="M8 0C3.58 0 0 3.58 0 8c0 3.54 2.29 6.53 5.47 7.59.4.07.55-.17.55-.38 0-.19-.01-.82-.01-1.49-2.01.37-2.53-.49-2.69-.94-.09-.23-.48-.94-.82-1.13-.28-.15-.68-.52-.01-.53.63-.01 1.08.58 1.23.82.72 1.21 1.87.87 2.33.66.07-.52.28-.87.51-1.07-1.78-.2-3.64-.89-3.64-3.95 0-.87.31-1.59.82-2.15-.08-.2-.36-1.02.08-2.12 0 0 .67-.21 2.2.82.64-.18 1.32-.27 2-.27.68 0 1.36.09 2 .27 1.53-1.04 2.2-.82 2.2-.82.44 1.1.16 1.92.08 2.12.51.56.82 1.27.82 2.15 0 3.07-1.87 3.75-3.65 3.95.29.25.54.73.54 1.48 0 1.07-.01 1.93-.01 2.2 0 .21.15.46.55.38A8.012 8.012 0 0 0 16 8c0-4.42-3.58-8-8-8z"/>
            </svg></a></li>
            </ul>
        </footer>
      </div>
    </div>
  </body>
</html>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
<script>
  $(document).ready(function () {
    $(".loader").hide();
    $("#dataGroup").children().hide();
    $("#dataGroup2").children().hide();

    // when Check Speed btn is clicked
    $("#check").on("click", function () {
      console.log("clicked");

      $("#dataGroup").children().hide();
      $("#dataGroup2").children().hide();
      $(".loader").fadeIn();

      $.ajax({
        method: "GET",
        url: "/checkspeed",
        success: function (data) {
          var upload = data.upspeed;
          var download = data.downspeed;
          var ping = data.ping.avg;
          var packetLoss = data.ping.loss;
          // var profile = data.profile;
          // var ISP = data.ISP;

          console.log(data);
          $(".loader").fadeOut(200);

          $("#upSpeed").fadeIn(1000);
          $("#upSpeed > span").text(upload + " mbps");

          $("#downSpeed").fadeIn(2000);
          $("#downSpeed > span").text(download + " mbps");

          $("#ping").fadeIn(3000);
          $("#ping > span").text(parseFloat(ping).toFixed(2) + " ms");

          $("#packetLoss").fadeIn(4000);
          $("#packetLoss > span").text(packetLoss);

          // $("#profile").fadeIn(5000);
          // $("#profile > div > div > .card-title").text(profile.isp_name);
          // $("#profile > div > div > p").text(profile.ip_address);

          // $("#isp").fadeIn(6000);
          // $("#isp > div > div > .card-title").text(ISP.name);
          // $("#isp > div > div > p").html(
          //   "<center>" +
          //     ISP.city +
          //     "<br>" +
          //     ISP.region +
          //     "<br>" +
          //     ISP.country +
          //     "</center>"
          // );
        },
      });
    });
  });
</script>
